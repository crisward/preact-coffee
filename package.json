{
  "name": "preact-coffee",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watchify src/index.cjsx -o build/index.js",
    "build": "browserify src/index.cjsx -d -p [minifyify --uglify [--mangle 0] --map build/index.map.json --output build/index.map.json]  -o build/index.js"
  },
  "browserify": {
    "debug": true,
    "transform": [
      [
        "coffee-reactify",
        {
          "extension": ".cjsx",
          "type": "coffee",
          "template": "jade"
        }
      ],
      [
        "coffeeify",
        {
          "extension": ".coffee"
        }
      ],
      [
        "aliasify",
        {
          "aliases": {
            "react": "preact-compat",
            "react-dom": "preact-compat"
          },
          "appliesTo": {
            "includeExtensions": [
              ".cjsx"
            ]
          },
          "verbose": false
        }
      ]
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "aliasify": "^2.1.0",
    "browserify": "^16.0.1",
    "coffee-reactify": "^5.1.0",
    "coffeeify": "^3.0.1",
    "coffeescript": "^2.3.1",
    "minifyify": "^7.3.5",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "proptypes": "^1.1.0",
    "watchify": "^3.11.0"
  }
}
