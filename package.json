{
  "name": "preact-coffee",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch --mode development",
    "build": "NODE_ENV=production webpack --mode production"
  },
  "browserify": {
    "debug": true,
    "transform": [
      [
        "coffee-reactify",
        {
          "extension": ".cjsx",
          "type": "coffee",
          "template": "jade"
        }
      ],
      [
        "coffeeify",
        {
          "extension": ".coffee"
        }
      ],
      [
        "aliasify",
        {
          "aliases": {
            "react": "preact-compat",
            "react-dom": "preact-compat"
          },
          "appliesTo": {
            "includeExtensions": [
              ".cjsx"
            ]
          },
          "verbose": false
        }
      ]
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.4.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "preact": "^10.0.5",
    "source-map-loader": "^0.2.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12"
  }
}
